# Generated by parallelRemake::write_makefile() on 2016-06-07 21:16:01 

.PHONY: all data.csv processed1 processed2 plot1.pdf plot2.pdf clean

all: plot1.pdf plot2.pdf 

data.csv:  
	Rscript -e 'remake::make("data.csv", remake_file = "remake.yml")'

processed1: data.csv 
	Rscript -e 'remake::make("processed1", remake_file = "remake.yml")'

processed2: data.csv 
	Rscript -e 'remake::make("processed2", remake_file = "remake.yml")'

plot1.pdf: processed1 
	Rscript -e 'remake::make("plot1.pdf", remake_file = "remake.yml")'

plot2.pdf: processed2 
	Rscript -e 'remake::make("plot2.pdf", remake_file = "remake.yml")'

clean:
	Rscript -e 'remake::make("clean", remake_file = "remake.yml")'
	rm -rf .remake
